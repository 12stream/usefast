
/*player*/
.player{
  position: fixed;
  display: flex;
  flex-flow: row;
  align-items: center;
  bottom: 0;
  left: 0;
  right: 0;
  height: 0;
  background-color: rgba(241,241,241,0.93);
  /* box-shadow: 0 -1px 0 0 rgba(0,0,0,.05); */
  padding: 0 0 0 26rpx;
  z-index: 999;
  transition: all 0.2s ease-out;
  /*-webkit-filter: blur(3px);
  filter: blur(3px);*/
  border-top:1rpx solid #ccc;
    /* border-bottom:1rpx solid #ccc; */
    opacity:0;
  overflow: hidden;
}
.player .player-lt{
  width: 78rpx;
  height: 78rpx;
  border-radius: 50%;
}
.player-dh{
  animation: player-animation 1200s linear infinite;
}
@keyframes  player-animation{
  0%{
  }
  100%{
    transform: rotateX(0deg) rotateY(0deg) rotateZ(70000deg);
    transform-origin: center center;
  }
}
.player-paused{
  -webkit-animation-play-state: paused;
  -moz-animation-play-state: paused;
  -o-animation-play-state: paused;
  animation-play-state: paused;
}
.player .player-rt{
  flex: 1;
  margin-left: 16rpx;
  overflow:hidden;
  height: 104rpx;
}
.player .player-name{
  display: inline-block;
  width: 90%;
  font-size: 28rpx;
  text-align: left;
  color: #333;
  line-height: 100%;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.player .slider-jd{
  position: relative;
  width: 488rpx;
  height: 18rpx;
}
.player .wx-slider-handle{
  position: absolute;
  top: 50%;
  width: 6rpx;
  height: 2rpx;
  margin-top: -1rpx;
  margin-left: -4rpx;
  background: #ff6704;
 
}
.player .slider{
  position: absolute;
  top: -8rpx;
  left: 0;
  width: 488rpx;
  height: 4rpx;
  display: block;
  margin: 0;
}

.player .player-time{
  font-size: 24rpx;
  text-align: left;
  color: #666;
  line-height: 100%;
}
.player-title{
  padding-top: 12rpx;
  padding-bottom:4rpx;
}
.player-pause-box-box{
  width: 64rpx;
  height: 64rpx;
  display: flex;
  align-items: center;
  padding-right: 26rpx;
  position: relative;
}
.player-pause-box{
  /*height: 102rpx;*/
  display: flex;
  align-items: center;
  position: absolute;
  /*right: 26rpx;*/
  /* z-index: 3; */
}

.player-play,.player-pause{
  /* position: absolute; */
  /* top: 17rpx; */
  /* right: 32rpx; */
  width: 64rpx;
  height: 64rpx;
  line-height: 64rpx;
  border: solid 3rpx #ababab;
  border-radius: 50%; 
  box-sizing: border-box;
  font-size: 24rpx;
  color: #ff6704;
  text-align: center;
}

/*åœ†*/
.circleProgress_wrapper{
  width: 64rpx;
  height: 64rpx;
  position: absolute;
  z-index: 3;
}

.wrapper{
  width: 32rpx;
  height: 64rpx;
  position: absolute;
  top:0;
  overflow: hidden;
}
.wrapper-right{
  /*right:0;*/
  left:32rpx;
}
.wrapper-left{
  left:1rpx;
}
.circleProgress{
  box-sizing: border-box;
  width: 64rpx;
  height: 64rpx;
  border:4rpx solid #ff6704;
  border-radius: 50%;
  position: absolute;
  top:0;
  transform: rotate(45deg);
}
.rightcircle{
  border-top:4rpx solid transparent;
  border-right:4rpx solid transparent;
  right:0;
  transform: rotate(45deg);
}
.leftcircle{
  border-bottom:4rpx solid transparent;
  border-left:4rpx solid transparent;
  left:0;
}

