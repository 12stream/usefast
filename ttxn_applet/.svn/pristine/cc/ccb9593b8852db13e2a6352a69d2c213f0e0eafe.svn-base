@import "../common/player.wxss";

page {
    background-color: #f4f4f4;
}

.container_box {
    padding-bottom: 16rpx;
}

.index-header {
    height: 72rpx;
    line-height: 72rpx;
    font-size: 28rpx;
    text-align: left;
    color: #fff;
    background: #ff6704;
}

.index-header .icon-rili {
    font-size: 32rpx;
    margin: 0 4rpx 0 32rpx;
}

.index-banner {
    position: relative;
    height: 296rpx;
    background: #fff;
}

.index-banner swiper {
    width: 100%;
    height: 100%;
}

.index-swiper .wx-swiper-dots.wx-swiper-dots-horizontal {
    margin-bottom: 6rpx;
}

.index-swiper .wx-swiper-dot {
    display: inline-flex;
    width: 24rpx;
    height: 4rpx;
    margin-left: 8rpx;
    justify-content: space-between;
    border-radius: 0;
}

.index-swiper .wx-swiper-dot::before {
    content: '';
    flex-grow: 1;
    border-radius: 0;
}

.index-banner image {
    width: 100%;
    height: 100%;
}

.xn-info {
    position: relative;
    background: #fff;
    padding-left: 26rpx;
    padding-bottom: 0.28rem;
}

.xn-info .xn-info-top {
    position: relative;
    height: 104rpx;
    line-height: 104rpx;
    font-size: 34rpx;
    font-weight: bold;
    text-align: left;
    color: #333;
    border-bottom: 1rpx solid #e5e5e5;
}

.xn-info .xn-info-top .info-play-btn {
    position: absolute;
    top: 20rpx;
    right: 26rpx;
    width: 214rpx;
    height: 64rpx;
    line-height: 64rpx;
    border-radius: 32rpx;
    font-size: 30rpx;
    font-weight: normal;
    color: #fff;
    background-color: #ff6704;
    box-shadow: 0px 4rpx 8rpx 0 rgba(255, 103, 4, 0.5);
    text-align: center;
    box-sizing: border-box;
}

.xn-info .xn-info-top .info-play-lxicon {
    display: inline-block;
    font-size: 24rpx;
    background-size: contain;
    margin-right: 16rpx;
}

.xn-info .xn-info-title {
    position: relative;
    height: 112rpx;
    line-height: 112rpx;
    overflow: hidden;
}

.none {
    display: none !important;
}

.xn-info .xn-info-title image {
    width: 64rpx;
    height: 64rpx;
    border-radius: 50%;
    background: #eee;
    vertical-align: top;
    margin-top: 24rpx;
}

.xn-info .xn-info-title text {
    font-size: 32rpx;
    font-weight: bold;
    text-align: left;
    color: #333;
    margin-left: 22rpx;
}

.xn-info .xn-info-title .more-btn {
    float: right;
    font-size: 28rpx;
    text-align: left;
    color: #999;
    margin-right: 26rpx;
}

.right-jt {
    display: inline-block;
    font-size: 22rpx;
    margin-left: 4rpx;
    color: #999;
    vertical-align: top;
}

.xn-info-list .info-list {
    height: 40rpx;
    line-height: 40rpx;
    font-size: 30rpx;
    text-align: left;
    color: #666;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    margin-bottom: 16rpx;
    padding-right: 24rpx;
}

.xn-info-list .info-list .play-icon {
    float: left;
    width: 40rpx;
    height: 40rpx;
    border: solid 2rpx #ccc;
    border-radius: 50%;
    box-sizing: border-box;
    margin-right: 16rpx;
    font-size: 18rpx;
    color: #ccc;
    text-align: center;
}

.xn-info-list .play-on, .xn-info-list .play-on .play-icon {
    color: #ff6704;
}

.xn-info-list .play-on .play-icon {
    border: solid 2rpx rgba(255, 103, 4, 0.3);
}

.xn-info-list .play-on .icon-bofang:before {
    content: '';
    display: inline-block;
    width: 14rpx;
    height: 16rpx;
    background: url(http://ttxuenong.oss-cn-shenzhen.aliyuncs.com/static/img/applet/9b178c20567645e984fc57c9977ab08c.gif) no-repeat;
    background-size: contain;
}

.sch-k {
    margin-left: 16rpx;
}

.xn-sch, .xn-things {
    position: relative;
    background: #fff;
    /* margin-top: 24rpx; */
    padding-left: 26rpx;
}

.index-h1 {
    position: relative;
    height: 88rpx;
    line-height: 88rpx;
    font-size: 34rpx;
    font-weight: bold;
    text-align: left;
    color: #333;
    border-bottom: 1rpx solid #e5e5e5;
}

.xn-sch .xn-sch-top .sch-change-btn {
    position: absolute;
    top: 20rpx;
    right: 32rpx;
    width: 166rpx;
    line-height: 42rpx;
    border-radius: 24rpx;
    font-size: 28rpx;
    color: #666;
    border: solid 1rpx #ccc;
    box-sizing: border-box;
    font-weight: normal;
}

.xn-sch .xn-sch-top .sch-change-btn text {
    position: absolute;
    width: 100rpx;
    height: 40rpx;
    top: 50%;
    right: 6rpx;
    display: inline-block;
    margin-top: -20rpx;
}

.xn-sch .xn-sch-top .sch-change-btn .change-ic {
    display: inline-block;
    font-size: 24rpx;
    color: #666;
    background-size: contain;
    margin-right: 9rpx;
    vertical-align: top;
    margin-top: 4.5rpx;
    margin-left: 18rpx;
}

.xn-sch .sch-list {
    position: relative;
    height: 249rpx;
    display: flex;
    flex-flow: row;
    align-items: center;
    border-bottom: 1rpx solid #e5e5e5;
    box-sizing: border-box;
}

.xn-sch .sch-list .sch-img {
    width: 160rpx;
    height: 200rpx;
    position: relative;
}

.xn-sch .sch-list .sch-img image {
    width: 100%;
    height: 100%;
    border-radius: 8rpx;
}

.xn-sch .sch-list .sch-list-info {
    flex: 1;
    overflow: hidden;
    padding: 0 26rpx 0 26rpx;
    line-height: 1.5;
}

.xn-sch .sch-list .sch-list-info .sch-h1 {
    display: block;
    font-size: 32rpx;
    font-weight: bold;
    text-align: left;
    color: #444;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.xn-sch .sch-list .sch-list-info .sch-p {
    display: block;
    font-size: 28rpx;
    text-align: left;
    color: #666;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    margin-top: 6rpx;
}

.sch-list .sch-list-info .sch-time, .sch-list .sch-list-info .sch-times {
    font-size: 26rpx;
    text-align: left;
    color: #999;
    margin-top: 13rpx;
}

.sch-m {
    font-size: 28rpx;
}

.sch-list .sch-list-info .sch-time {
    margin: 6rpx 0 6rpx;
}

.sch-list-info .sch-times .sch-buy {
    margin-top: 24rpx;
}

.sch-list-info .sch-times .sch-buy .o-price {
    text-decoration: line-through;
    margin-left: 19rpx;
}

.sch-list-info .sch-times .sch-buy .n-price {
    font-size: 30rpx;
    font-weight: bold;
    text-align: left;
    color: #ff6704;
    /* float:right; */
    display: block;
}

.xn-sch .sch-bott {
    position: relative;
    height: 72rpx;
    padding-top: 24rpx;
    color: #666;
    font-size: 28rpx;
}

.xn-sch .sch-bott .sch-more-btn {
    position: relative;
    font-size: 14px;
    color: #333;
    text-align: center;
    margin: 0 auto;
}

.xn-sch .sch-bott .sch-more-btn .btn-all {
    padding: 2px 10px;
    border: solid 1rpx #ccc;
    box-sizing: border-box;
    border-radius: 12px;
}

.xn-sch .sch-bott .sch-more-btn .right-jt text {
    position: absolute;
    top: 50%;
    left: 412rpx;
    height: 24rpx;
    font-size: 24rpx;
    margin-top: -10rpx;
}

.xn-things {
    height: 392rpx;
}

.xn-things .xn-th-list {
    position: relative;
    float: left;
    width: 208rpx;
    text-align: center;
    margin-left: 32rpx;
    margin-top: 24rpx;
}

.xn-things  .xn-th-1 {
    margin-left: 0;
}

.xn-things .xn-th-list image {
    position: relative;
    width: 100%;
    height: 208rpx;
    border-radius: 8rpx;
}

.xn-things .xn-th-list .p-label {
    display: inline-block;
    font-size: 28rpx;
    text-align: left;
    color: #666;
    padding-bottom: 22rpx;
}

.xn-things .xn-th-list .play-bar {
    position: absolute;
    bottom: 16rpx;
    left: 16rpx;
    display: inline-block;
    width: 56rpx;
    height: 56rpx;
    background: rgba(0, 0, 0, 0.6);
    border: solid 2rpx rgba(255, 255, 255, 0.3);
    border-radius: 50%;
    text-align: center;
    box-sizing: border-box;
}

.xn-things .xn-th-list .play-bar label {
    float: left;
    font-size: 26rpx;
    color: #fff;
    margin-left: 11rpx;
    margin-top: 12rpx;
}

.index-header-rt {
    float: right;
    margin-right: 28rpx;
}

.sch-change-btn {
    text-align: center;
}

.sch-more-btn2 {
    position: absolute;
    right: 26rpx;
    top: 0rpx;
    color: #999;
    font-size: 28rpx;
    font-weight: normal;
}

/* // canvas */

page {
    width: 100%;
    height: 100%;
    background-color: #fff;
}

.circle-box {
    text-align: center;
    margin-top: 10vw;
}

.circle {
    position: absolute;
    left: 0;
    right: 0;
    margin: auto;
}

.draw_btn {
    width: 35vw;
    position: absolute;
    top: 104vw;
    right: 0;
    left: 0;
    margin: auto;
    border: 1px #000 solid;
    border-radius: 5vw;
}

.wrap {
    position: absolute;
    top: 50%;
}

image.zuixin {
    position: absolute;
    top: 0;
    left: 0;
    height: 57rpx !important;
    width: 57rpx !important;
}

.load-cur {
    position: absolute;
    right: -8rpx;
    top: -8rpx;
    height: 20rpx;
    width: 20rpx;
    border-radius: 50%;
    background-color: #ff3b30;
}

.bd-ra {
    display: inline-block;
    border: 1rpx solid #ff3b30;
    color: #ff3b30;
    border-radius: 20rpx;
    margin-left: 16rpx;
    padding: 3rpx 10rpx;
    font-size: 20rpx;
    height: 28rpx;
    line-height: 28rpx;
}
