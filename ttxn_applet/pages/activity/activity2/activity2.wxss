page {
    display: block;
    min-height: 100%;
    background-color: #f4f4f4;
}
.banner{
    width: 100%;
    height: 240rpx;
    background-size:contain;
}
.ac-con{
    /* justify-content: space-around; */
    flex-wrap:wrap;
    /*padding-left:74rpx;*/
    padding-right:50rpx;
    margin-left:24rpx;
    align-items: center;
}
.ac-cell{
    /*border:1px solid red;*/
    width: 88rpx;
    font-size:24rpx;
    color:#666;
    text-align: center;
    /*overflow: hidden;*/
    position: relative;
    margin-left:24rpx;
    margin-bottom:24rpx;
}
.ac-img{
    width: 88rpx;
    height: 88rpx;
    border-radius: 88rpx;
    margin-bottom: 12rpx;
}
.ac-img-qt{
    width: 88rpx;
    height: 88rpx;
    line-height:88rpx;
    border-radius: 88rpx;
    margin-bottom: 12rpx;
    background-color: #fff;
    display: inline-block;
    color:#ff6704;
}
.ac-cell-xz{
    display: none;
    background-color: rgba(255,103,4,0.3);
    color:#fff;
    text-align: center;
    width: 88rpx;
    height: 88rpx;
    line-height: 88rpx;
    font-size:40rpx;
    border-radius: 88rpx;
    position: absolute;
    top: 0;
    left: 0;
    border:1px solid #ff6704;
    box-sizing: border-box;
}
.active .ac-cell-xz{
    display: block;
}
.ac-hide{
    display: none;
}